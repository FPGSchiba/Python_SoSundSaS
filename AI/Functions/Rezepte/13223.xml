   <einkaufliste>
      <Spinatfuellung>
          <quant>1/2 EL</quant>
          <zutat>Olivenoel</zutat>
          <quant>1 </quant>
          <zutat>Zwiebel, fein geschnitten</zutat>
          <quant>1 </quant>
