<?xml version="1.0" encoding="UTF-8"?>
<Rezept>
   <meta>
       <NaehrwertProPerson>691 kcal</NaehrwertProPerson>
       <Fett>47 g  </Fett>
       <Kohlenhydrate>21 g  </Kohlenhydrate>
       <Eiweiss>43 g  </Eiweiss>
   </meta>
   <einkaufliste>
      <Suppe     >
          <quant>5 dl</quant>
          <zutat>Kokosmilch</zutat>
          <quant>2 dl</quant>
          <zutat>Wasser</zutat>
          <quant>2 EL</quant>
          <zutat>gruene Currypaste</zutat>
          <quant>1/2 TL</quant>
          <zutat>Salz</zutat>
          <quant>280 g</quant>
          <zutat>Pouletbrustfilets</zutat>
      </Suppe     >
      <Kefen     >
          <quant>250 g</quant>
          <zutat>Kefen, schraeg in Streifen</zutat>
      </Kefen     >
   </einkaufliste>
   <rezept>
      <Suppe     >
          <inhalt>Kokosmilch und Wasser mit Currypaste aufkochen, salzen. Poulet beigeben, zugedeckt ca. 15 Min. koecheln. Poulet herausnehmen, mit zwei Gabeln zerzupfen.</inhalt>
      </Suppe     >
      <Kefen     >
          <inhalt>Poulet mit den Kefen zur Suppe geben, ca. 5 Min. fertig koecheln. </inhalt>
      </Kefen     >
   </rezept>
</Rezept>