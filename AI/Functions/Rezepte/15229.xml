<Rezept>
   <meta>
       <NaehrwertProPerson>383 kcal</NaehrwertProPerson>
       <Fett>13 g  </Fett>
       <Kohlenhydrate>39 g  </Kohlenhydrate>
       <Eiweiss>24 g  </Eiweiss>
   </meta>
   <einkaufliste>
      <Kartoffeln>
          <quant>1/2 TL</quant>
          <zutat>Salz</zutat>
          <quant>1 EL</quant>
          <zutat>Olivenoel</zutat>
          <quant>2 Zweiglein</quant>
          <zutat>Rosmarin</zutat>
          <quant>800 g</quant>
          <zutat>Baby-Kartoffeln, in Schnitzen</zutat>
      </Kartoffeln>
      <Pouletfluegeli>
          <quant>2 EL</quant>
          <zutat>Honig</zutat>
          <quant>1 </quant>
          <zutat>Knoblauchzehe, gepresst</zutat>
          <quant>1 TL</quant>
