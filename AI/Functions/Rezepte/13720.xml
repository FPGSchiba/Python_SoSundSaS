          <zutat>Salz</zutat>
      </Salat     >
      <Sauce_    >
          <quant>1 1/2 EL</quant>
          <zutat>Zitronensaft</zutat>
          <quant>2 EL</quant>
          <zutat>Olivenoel</zutat>
          <quant>2 EL</quant>
          <zutat>glattblaettrige Petersilie, fein geschnitten</zutat>
          <quant>2 EL</quant>
          <zutat>Pfefferminze, fein geschnitten</zutat>
