<?xml version="1.0" encoding="UTF-8"?>
<Rezept>
   <meta>
       <NaehrwertProPerson>398 kcal</NaehrwertProPerson>
       <Fett>32 g  </Fett>
       <Kohlenhydrate>3 g   </Kohlenhydrate>
       <Eiweiss>24 g  </Eiweiss>
   </meta>
   <einkaufliste>
      <Lachs_    >
          <quant>1 </quant>
          <zutat>Bio-Zitrone</zutat>
          <quant>1 </quant>
          <zutat>Vanilleschote</zutat>
          <quant>450 g</quant>
          <zutat>Lachsfilets ohne Haut </zutat>
          <quant>1 TL</quant>
          <zutat>Fenchelsamen</zutat>
      </Lachs_    >
      <Garen_im_Ofen>
          <quant>5 dl</quant>
          <zutat>Olivenoel (nicht kaltgepresst)</zutat>
      </Garen_im_Ofen>
      <Mandelstreusel>
          <quant>3 EL</quant>
          <zutat>Mandeln</zutat>
          <quant>1/2 Bund</quant>
          <zutat>Dill</zutat>
      </Mandelstreusel>
      <Fenchelsalat>
          <quant>1 </quant>
          <zutat>Fenchel</zutat>
          <quant>2 EL</quant>
          <zutat>Zitronensaft</zutat>
          <quant>2 EL</quant>
          <zutat>Olivenoel</zutat>
          <quant>1/4 TL</quant>
          <zutat>Salz</zutat>
          <quant>wenig </quant>
          <zutat>tasmanischer Pfeffer</zutat>
      </Fenchelsalat>
      <Anrichten_>
          <quant>1/2 TL</quant>
          <zutat>Fleur de Sel</zutat>
      </Anrichten_>
   </einkaufliste>
   <rezept>
      <How_To    >
          <inhalt>Lachs                         </inhalt>
      </How_To    >
      <Lachs_    >
          <inhalt>Garen im Ofen                 </inhalt>
      </Lachs_    >
      <Garen_im_Ofen>
          <inhalt>Mandelstreusel                </inhalt>
      </Garen_im_Ofen>
      <Mandelstreusel>
          <inhalt> 2 EL Oel (vom Confieren) in einer Bratpfanne erhitzen. Mandeln, gehackte Zitrone und Dill beigeben und ca. 3 Min. ruehrbraten, beiseite stellen. </inhalt>
      </Mandelstreusel>
      <Fenchelsalat>
          <inhalt>Fenchel fein hobeln. Zitronensaft und Oel verruehren, wuerzen. Fenchel beigeben, mischen. </inhalt>
      </Fenchelsalat>
      <Anrichten_>
          <inhalt>Fisch sorgfaeltig aus dem Oel nehmen, salzen. Fisch mit den Zitronenscheiben auf dem Fenchelsalat anrichten, Mandelstreusel darueberstreuen. </inhalt>
      </Anrichten_>
   </rezept>
</Rezept>