       <Eiweiss>43 g  </Eiweiss>
   </meta>
   <einkaufliste>
      <Liste     >
          <quant>1 </quant>
          <zutat>Weissbrot (z.B. Sunnebrot, ca. 370 g)</zutat>
          <quant>1 EL</quant>
          <zutat>Preiselbeerkonfituere</zutat>
          <quant>1 EL</quant>
          <zutat>Baumnuesse, grob gehackt</zutat>
          <quant>40 g</quant>
          <zutat>Speckwuerfeli</zutat>
          <quant>200 g</quant>
          <zutat>Raclettekaese, in Scheiben, halbiert</zutat>
          <quant>wenig </quant>
