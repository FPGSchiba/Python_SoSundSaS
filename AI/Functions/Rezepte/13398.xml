<?xml version="1.0" encoding="UTF-8"?>
<Rezept>
   <meta>
       <NaehrwertProPerson>75 kcal </NaehrwertProPerson>
       <Fett>6 g   </Fett>
       <Kohlenhydrate>3 g   </Kohlenhydrate>
       <Eiweiss>1 g   </Eiweiss>
   </meta>
   <einkaufliste>
      <Dressing  >
          <quant>3 EL</quant>
          <zutat>Mayonnaise</zutat>
          <quant>1 EL</quant>
          <zutat>Zitronensaft</zutat>
          <quant>1 EL</quant>
          <zutat>Sojasauce</zutat>
          <quant>1 TL</quant>
          <zutat>Wasabi-Paste</zutat>
          <quant>1/2 TL</quant>
          <zutat>Fischsauce</zutat>
          <quant>1 EL</quant>
          <zutat>Sesam</zutat>
      </Dressing  >
      <Coleslaw  >
          <quant>150 g</quant>
          <zutat>Weisskabis, fein gehobelt</zutat>
          <quant>100 g</quant>
          <zutat>Rueebli, in feinen Streifen</zutat>
          <quant>100 g</quant>
          <zutat>Stangensellerie, in feinen Scheiben</zutat>
          <quant>100 g</quant>
          <zutat>Rettich, in feinen Streifen</zutat>
          <quant>1 </quant>
          <zutat>Bundzwiebel mit dem Gruen, fein geschnitten</zutat>
          <quant>1 Bund</quant>
          <zutat>Koriander, fein zerzupft</zutat>
      </Coleslaw  >
   </einkaufliste>
   <rezept>
      <Dressing  >
          <inhalt>Mayonnaise und alle Zutaten bis und mit Sesam in einer Schuessel gut verruehren. </inhalt>
      </Dressing  >
      <Coleslaw  >
          <inhalt>Kabis, Rueebli, Stangensellerie und Rettich mit dem Dressing mischen, zugedeckt bei Raumtemperatur ca. 40 Min. ziehen lassen. Bundzwiebel und Koriander beigeben, mischen.</inhalt>
      </Coleslaw  >
   </rezept>
</Rezept>