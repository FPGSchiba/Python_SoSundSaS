   </meta>
   <einkaufliste>
      <Baked_Potatoes_>
          <quant>1 kg</quant>
          <zutat>Kartoffel Baked Potatoe</zutat>
          <quant>1 EL</quant>
          <zutat>Olivenoel</zutat>
          <quant>1 TL</quant>
          <zutat>Fleur de Sel</zutat>
      </Baked_Potatoes_>
      <Backen_   >
          <quant>200 g</quant>
          <zutat>Tomatensauce</zutat>
          <quant>150 g</quant>
          <zutat>Mozzarella, in Wuerfeli</zutat>
          <quant>60 g</quant>
          <zutat>Salami, in Tranchen</zutat>
      </Backen_   >
