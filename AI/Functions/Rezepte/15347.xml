<?xml version="1.0" encoding="UTF-8"?>
<Rezept>
   <meta>
       <NaehrwertProPerson>570 kcal</NaehrwertProPerson>
       <Fett>40 g  </Fett>
       <Kohlenhydrate>35 g  </Kohlenhydrate>
       <Eiweiss>15 g  </Eiweiss>
   </meta>
   <einkaufliste>
      <Teigboden >
          <quant>250 g</quant>
          <zutat>Blaetterteig</zutat>
      </Teigboden >
      <Quiches   >
          <quant>250 g</quant>
          <zutat>gruene Spargeln, unteres Drittel geschaelt, in feinen Scheiben</zutat>
          <quant>1 </quant>
          <zutat>Schalotte, fein gehackt</zutat>
          <quant>1 EL</quant>
          <zutat>Butter</zutat>
          <quant>1/2 dl</quant>
          <zutat>Aceto balsamico bianco</zutat>
          <quant>2 </quant>
          <zutat>Eier</zutat>
          <quant>1 dl</quant>
          <zutat>Vollrahm</zutat>
          <quant>2 EL</quant>
          <zutat>Meerrettich, fein gerieben</zutat>
          <quant>1/2 TL</quant>
          <zutat>Fleur de Sel</zutat>
          <quant>wenig </quant>
          <zutat>Pfeffer</zutat>
      </Quiches   >
      <Belag     >
          <quant>160 g</quant>
          <zutat>Rauchlachs</zutat>
          <quant>wenig </quant>
          <zutat>Meerrettich</zutat>
          <quant>wenig </quant>
          <zutat>Dill</zutat>
      </Belag     >
      <Dip       >
          <quant>3 EL</quant>
          <zutat>Creme fraiche</zutat>
          <quant>2 EL</quant>
          <zutat>Olivenoel mit Zitrone</zutat>
          <quant>1 EL</quant>
          <zutat>Rapsoel</zutat>
          <quant>1 EL</quant>
          <zutat>Dill, fein geschnitten</zutat>
          <quant>wenig </quant>
          <zutat>Salz</zutat>
          <quant>wenig </quant>
          <zutat>Pfeffer</zutat>
      </Dip       >
   </einkaufliste>
   <rezept>
      <How_To    >
          <inhalt>Teigboden                     </inhalt>
      </How_To    >
      <Teigboden >
          <inhalt>Quiches                       </inhalt>
      </Teigboden >
      <Quiches   >
          <inhalt>Backen: ca. 30 Min. in der unteren Haelfte des auf 180 Grad vorgeheizten Ofens. Quiches herausnehmen, aus den Foermchen nehmen, auf einem Gitter etwas abkuehlen. </inhalt>
      </Quiches   >
      <Belag     >
          <inhalt>Quiches mit Lachs belegen, Meerretttich darueberreiben, mit Dill garnieren.</inhalt>
      </Belag     >
      <Dip       >
          <inhalt>Creme fraiche und alle Zutaten bis und mit Dill verruehren, wuerzen, zu den Quiches servieren.</inhalt>
      </Dip       >
   </rezept>
</Rezept>