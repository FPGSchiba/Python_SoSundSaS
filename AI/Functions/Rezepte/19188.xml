   </meta>
   <einkaufliste>
      <Marinade  >
          <quant>2 EL</quant>
          <zutat>Honigsenf</zutat>
          <quant>3 EL</quant>
          <zutat>Olivenoel</zutat>
          <quant>2 EL</quant>
          <zutat>Aprikosenkonfituere</zutat>
          <quant>1/2 TL</quant>
          <zutat>Chiliflocken</zutat>
          <quant>1 </quant>
          <zutat>Bio-Zitrone, wenig abgeriebene Schale, nur 1 EL Saft</zutat>
          <quant>1 TL</quant>
          <zutat>Salz</zutat>
          <quant>4 </quant>
