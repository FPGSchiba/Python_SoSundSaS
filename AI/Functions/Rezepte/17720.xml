<?xml version="1.0" encoding="UTF-8"?>
<Rezept>
   <meta>
       <NaehrwertProPerson>273 kcal</NaehrwertProPerson>
       <Fett>17 g  </Fett>
       <Kohlenhydrate>19 g  </Kohlenhydrate>
       <Eiweiss>7 g   </Eiweiss>
   </meta>
   <einkaufliste>
      <Geroesteter_Fenchel>
          <quant>3 </quant>
          <zutat>Fenchel, fein gehobelt</zutat>
          <quant>2 EL</quant>
          <zutat>Olivenoel</zutat>
          <quant>1/2 TL</quant>
          <zutat>Salz</zutat>
      </Geroesteter_Fenchel>
      <Sauce     >
          <quant>2 EL</quant>
          <zutat>Zitronensaft</zutat>
          <quant>2 EL</quant>
          <zutat>Olivenoel</zutat>
          <quant>1/4 TL</quant>
          <zutat>Salz</zutat>
          <quant>wenig </quant>
          <zutat>Pfeffer</zutat>
      </Sauce     >
      <Salat     >
          <quant>2 </quant>
          <zutat>Nektarinen, in Schnitzen</zutat>
          <quant>2 </quant>
          <zutat>Pfirsiche, in Schnitzen</zutat>
          <quant>100 g</quant>
          <zutat>Mozzarelline</zutat>
          <quant>2 EL</quant>
          <zutat>Basilikum, zerzupft</zutat>
      </Salat     >
   </einkaufliste>
   <rezept>
      <Geroesteter_Fenchel>
          <inhalt>3/4 des Fenchels auf ein mit Backpapier belegtes Blech verteilen, mit Oel betraeufeln, salzen. </inhalt>
      </Geroesteter_Fenchel>
      <Backen    >
          <inhalt>Ca. 45 Min. in der Mitte des auf 200 Grad vorgeheizten Ofens. Herausnehmen, auf einer Platte anrichten. </inhalt>
      </Backen    >
      <Sauce     >
          <inhalt>Zitronensaft und Oel verruehren, wuerzen. </inhalt>
      </Sauce     >
      <Salat     >
          <inhalt>Nektarinen, Pfirsiche, restlicher Fenchel und Mozzarelline auf dem Fenchel verteilen, mit der Sauce betraeufeln, Basilikum darueberstreuen. </inhalt>
      </Salat     >
   </rezept>
</Rezept>