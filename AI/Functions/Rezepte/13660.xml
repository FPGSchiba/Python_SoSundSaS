          <zutat>Olivenoel</zutat>
          <quant>1 </quant>
          <zutat>Zwiebel, fein gehackt</zutat>
          <quant>2 </quant>
          <zutat>Knoblauchzehen, fein gehackt</zutat>
          <quant>1 </quant>
          <zutat>Aubergine, in Wuerfeln</zutat>
          <quant>120 g</quant>
          <zutat>Edamame, ausgeloest</zutat>
          <quant>80 g</quant>
          <zutat>Jungspinat</zutat>
      </Gemuese    >
      <Spaghetti >
          <quant>180 g</quant>
          <zutat>Spaghetti (Soja-Vollkorn)</zutat>
          <quant> </quant>
          <zutat>Salzwasser, siedend</zutat>
      </Spaghetti >
      <Sauce_    >
          <quant>1/2 </quant>
