          <quant>2 EL</quant>
          <zutat>Sojasauce</zutat>
          <quant>1 EL</quant>
          <zutat>Fischsauce</zutat>
          <quant>2 </quant>
          <zutat>Knoblauchzehen, gepresst</zutat>
          <quant>1 EL</quant>
          <zutat>Ingwer, gerieben</zutat>
          <quant>1 EL</quant>
          <zutat>Rohzucker</zutat>
      </Marinade  >
      <Fleisch_  >
          <quant>500 g</quant>
          <zutat>Hackfleisch (Rind)</zutat>
          <quant>1/2 EL</quant>
          <zutat>Sesamoel</zutat>
          <quant>250 g</quant>
          <zutat>Mungbohnensprossen</zutat>
          <quant>1 </quant>
