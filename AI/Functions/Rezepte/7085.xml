          <zutat>Schwarztee (z.B. Earl Grey)</zutat>
          <quant>1/2 EL</quant>
          <zutat>Rohzucker</zutat>
          <quant>3/4 TL</quant>
          <zutat>grobkoerniges Meersalz</zutat>
          <quant>6 </quant>
          <zutat>Pfefferkoerner</zutat>
      </Fleisch   >
      <Sauce     >
          <quant>1 EL</quant>
          <zutat>Schwarztee (z.B. Earl Grey)</zutat>
          <quant>2 dl</quant>
          <zutat>Wasser, siedend</zutat>
          <quant>2 dl</quant>
          <zutat>roter Portwein</zutat>
          <quant>1 </quant>
          <zutat>Schalotte, fein gehackt</zutat>
