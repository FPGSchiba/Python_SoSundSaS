       <Kohlenhydrate>3 g   </Kohlenhydrate>
       <Eiweiss>34 g  </Eiweiss>
   </meta>
   <einkaufliste>
      <Fleisch   >
          <quant>2 </quant>
          <zutat>Lammracks (je ca. 400 g)</zutat>
          <quant>2 EL</quant>
          <zutat>Olivenoel zum Braten</zutat>
          <quant>1 1/4 TL</quant>
          <zutat>Salz</zutat>
          <quant>wenig </quant>
          <zutat>Pfeffer</zutat>
      </Fleisch   >
      <Gemuese    >
          <quant>1 1/2 dl</quant>
          <zutat>Gemuesebouillon</zutat>
          <quant>1 1/2 dl</quant>
          <zutat>Noilly Prat</zutat>
          <quant>einige </quant>
          <zutat>Safranfaeden</zutat>
          <quant>1/2 TL</quant>
          <zutat>schwarzer Pfeffer</zutat>
          <quant>1/4 TL</quant>
          <zutat>Salz</zutat>
          <quant>600 g</quant>
