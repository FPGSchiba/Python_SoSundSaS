<?xml version="1.0" encoding="UTF-8"?>
<Rezept>
   <meta>
       <NaehrwertProPerson>250 kcal</NaehrwertProPerson>
       <Fett>14 g  </Fett>
       <Kohlenhydrate>20 g  </Kohlenhydrate>
       <Eiweiss>11 g  </Eiweiss>
   </meta>
   <einkaufliste>
      <Blutwurstmasse>
          <quant>3 </quant>
          <zutat>Blutwuerste  (je ca. 100 g)</zutat>
          <quant>1 EL</quant>
          <zutat>Petersilie, fein geschnitten</zutat>
      </Blutwurstmasse>
      <Blutwurst-Pralinen>
          <quant>1 Pack</quant>
          <zutat>Strudelteig </zutat>
          <quant>20 g</quant>
          <zutat>Butter, fluessig</zutat>
      </Blutwurst-Pralinen>
      <Sauerkraut>
          <quant>500 g</quant>
          <zutat>gekochtes Sauerkraut, abgetropft, ausgedrueckt</zutat>
          <quant>1 dl</quant>
          <zutat>Apfelsaft</zutat>
          <quant>2 dl</quant>
          <zutat>Vollrahm, steif geschlagen</zutat>
          <quant>1 EL</quant>
          <zutat>Petersilie, geschnitten</zutat>
      </Sauerkraut>
      <Karamell-Marroni>
          <quant>50 g</quant>
          <zutat>Butter</zutat>
          <quant>80 g</quant>
          <zutat>Zucker</zutat>
          <quant>1 dl</quant>
          <zutat>Bier</zutat>
          <quant>500 g</quant>
          <zutat>tiefgekuehlte geschaelte Marroni, aufgetaut</zutat>
          <quant>1/4 TL</quant>
          <zutat>Salz</zutat>
          <quant>wenig </quant>
          <zutat>Pfeffer</zutat>
      </Karamell-Marroni>
   </einkaufliste>
   <rezept>
      <How_To    >
          <inhalt>Blutwurstmasse                </inhalt>
      </How_To    >
      <Blutwurstmasse>
          <inhalt>Blutwurst-Pralinen            </inhalt>
      </Blutwurstmasse>
      <Blutwurst-Pralinen>
          <inhalt>Backen                        </inhalt>
      </Blutwurst-Pralinen>
      <Backen    >
          <inhalt>Sauerkraut                    </inhalt>
      </Backen    >
      <Sauerkraut>
          <inhalt>Karamell-Marroni              </inhalt>
      </Sauerkraut>
      <Karamell-Marroni>
          <inhalt>Marroni, Sauerkraut und Pralinen zusammen servieren. </inhalt>
      </Karamell-Marroni>
   </rezept>
</Rezept>