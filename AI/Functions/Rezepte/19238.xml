<?xml version="1.0" encoding="UTF-8"?>
<Rezept>
   <meta>
       <NaehrwertProPerson>598 kcal</NaehrwertProPerson>
       <Fett>22 g  </Fett>
       <Kohlenhydrate>50 g  </Kohlenhydrate>
       <Eiweiss>9 g   </Eiweiss>
   </meta>
   <einkaufliste>
      <Kaesestaengel>
          <quant>4 Scheiben</quant>
          <zutat>Zopf</zutat>
          <quant>80 g</quant>
          <zutat>Gruyere, grob gerieben</zutat>
          <quant>2 EL</quant>
          <zutat>Butter, fluessig</zutat>
      </Kaesestaengel>
      <Suppe     >
          <quant>4 dl</quant>
          <zutat>Tomatensuppe</zutat>
      </Suppe     >
   </einkaufliste>
   <rezept>
      <How_To    >
          <inhalt>Kaesestaengel                   </inhalt>
      </How_To    >
      <Kaesestaengel>
          <inhalt>Kaesebrote in einer Grillpfanne oder beschichteten Bratpfanne beidseitig je ca. 2 1/2 Min. braten. Brote in ca. 2 cm dicke Staengel schneiden. </inhalt>
      </Kaesestaengel>
      <Suppe     >
          <inhalt>Suppe in einer Pfanne erwaermen. Kaesestaengel dazu servieren. </inhalt>
      </Suppe     >
   </rezept>
</Rezept>