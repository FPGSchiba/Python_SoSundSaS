<?xml version="1.0" encoding="UTF-8"?>
<Rezept>
   <meta>
       <NaehrwertProPerson>195 kcal</NaehrwertProPerson>
       <Fett>12 g  </Fett>
       <Kohlenhydrate>17 g  </Kohlenhydrate>
       <Eiweiss>3 g   </Eiweiss>
   </meta>
   <einkaufliste>
      <Aquafaba  >
          <quant>1 Dose</quant>
          <zutat>Kichererbsen (ca. 240 g), nur die Fluessigkeit</zutat>
      </Aquafaba  >
      <Teig      >
          <quant>160 g</quant>
          <zutat>Zucker</zutat>
          <quant>1/4 TL</quant>
          <zutat>gemahlene Bourbon-Vanille</zutat>
          <quant>1 Prise</quant>
          <zutat>Salz</zutat>
          <quant>300 g</quant>
          <zutat>dunkle Schokolade (vegan), gehackt</zutat>
          <quant>50 g</quant>
          <zutat>Kokosoel</zutat>
          <quant>200 g</quant>
          <zutat>Weissmehl</zutat>
          <quant>1 TL</quant>
          <zutat>Backpulver</zutat>
          <quant>100 g</quant>
          <zutat>Baumnusskerne, grob gehackt</zutat>
          <quant>100 g</quant>
          <zutat>Pekannuesse, grob gehackt</zutat>
      </Teig      >
   </einkaufliste>
   <rezept>
      <How_To    >
          <inhalt>Aquafaba                      </inhalt>
      </How_To    >
      <Aquafaba  >
          <inhalt>Teig                          </inhalt>
      </Aquafaba  >
      <Teig      >
          <inhalt>ca. 35 Min. in der Mitte des auf 180 Grad vorgeheizten Ofens. Herausnehmen, etwas abkuehlen, Formenrand entfernen, auskuehlen, in ca. 25 Wuerfel schneiden.</inhalt>
      </Teig      >
   </rezept>
</Rezept>