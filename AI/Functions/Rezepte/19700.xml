   <einkaufliste>
      <Dip       >
          <quant>150 g</quant>
          <zutat>griechischer Jogurt nature</zutat>
          <quant>125 g</quant>
          <zutat>Frischkaese nature</zutat>
          <quant>1 </quant>
          <zutat>Bio-Zitrone, wenig abgeriebene Schale</zutat>
          <quant>1 1/2 EL</quant>
          <zutat>Dukkah (Gewuerzmischung)</zutat>
      </Dip       >
      <Hackfleisch-Spiessli>
          <quant>600 g</quant>
          <zutat>Hackfleisch (Poulet)</zutat>
          <quant>6 EL</quant>
          <zutat>Panko oder Paniermehl</zutat>
          <quant>1 </quant>
          <zutat>Zwiebel, fein gehackt</zutat>
          <quant>1 </quant>
          <zutat>Knoblauchzehe, gepresst</zutat>
