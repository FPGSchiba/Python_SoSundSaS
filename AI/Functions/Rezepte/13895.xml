<?xml version="1.0" encoding="UTF-8"?>
<Rezept>
   <meta>
       <NaehrwertProPerson>546 kcal</NaehrwertProPerson>
       <Fett>8 g   </Fett>
       <Kohlenhydrate>91 g  </Kohlenhydrate>
       <Eiweiss>22 g  </Eiweiss>
   </meta>
   <einkaufliste>
      <Teig      >
          <quant>230 g</quant>
          <zutat>Dinkelmehl</zutat>
          <quant>1 TL</quant>
          <zutat>Salz</zutat>
          <quant>1/4 Wuerfel</quant>
          <zutat>Hefe, zerbroeckelt</zutat>
          <quant>1 1/2 dl</quant>
          <zutat>Wasser</zutat>
      </Teig      >
      <Hummus    >
          <quant>1 Dose</quant>
          <zutat>Kichererbsen, abgetropft (ca. 240 g)</zutat>
          <quant>2 </quant>
          <zutat>Bio-Zitronen, abgeriebene Schale und 2 EL Saft</zutat>
          <quant>1 TL</quant>
          <zutat>Salz</zutat>
          <quant>1 TL</quant>
          <zutat>Kreuzkuemmelpulver</zutat>
          <quant>1 TL</quant>
          <zutat>Za'Atar (Gewuerzmischung)</zutat>
          <quant>1 TL</quant>
          <zutat>Paprika</zutat>
          <quant>1 TL</quant>
          <zutat>Harissa</zutat>
          <quant>1 </quant>
          <zutat>Knoblauchzehe, gepresst</zutat>
      </Hummus    >
      <Belegen_&amp;_Backen>
          <quant>wenig </quant>
          <zutat>Dinkelmehl</zutat>
          <quant>2 </quant>
          <zutat>Randen (klein), in duennen Scheiben</zutat>
          <quant>1 1/2 EL</quant>
          <zutat>Pinienkerne</zutat>
      </Belegen_&amp;_Backen>
      <Kale/Federkohl>
          <quant>2 Handvoll</quant>
          <zutat>gewaschener, geruesteter Federkohl</zutat>
          <quant>2 EL</quant>
          <zutat>Tahina </zutat>
          <quant>2 EL</quant>
          <zutat>Zitronensaft</zutat>
          <quant>1 TL</quant>
          <zutat>Salz</zutat>
      </Kale/Federkohl>
   </einkaufliste>
   <rezept>
      <How_To    >
          <inhalt>Teig                          </inhalt>
      </How_To    >
      <Teig      >
          <inhalt>Hummus                        </inhalt>
      </Teig      >
      <Hummus    >
          <inhalt>Belegen &amp; Backen          </inhalt>
      </Hummus    >
      <Belegen_&amp;_Backen>
          <inhalt>Backen: ca. 10 Min. im unteren Drittel des auf 240 Grad vorgeheizten Ofens. Pinienkerne auf den Pizzen veritelen, 2 Min. fertigbacken.</inhalt>
      </Belegen_&amp;_Backen>
      <Kale/Federkohl>
          <inhalt>Federkohl mit Tahina, Zitronensaft und Salz einmassieren, auf den fertigen Pizzen verteilen.</inhalt>
      </Kale/Federkohl>
   </rezept>
</Rezept>