       <Eiweiss>51 g  </Eiweiss>
   </meta>
   <einkaufliste>
      <Fill_Header>
          <quant>800 g</quant>
          <zutat>Kalbshohruecken</zutat>
          <quant>1 </quant>
          <zutat>Zucchetti (ca. 200 g), fein gerieben</zutat>
          <quant>1/2 TL</quant>
          <zutat>Salz</zutat>
          <quant>1 </quant>
          <zutat>Knoblauchzehe, gepresst</zutat>
          <quant>2 EL</quant>
          <zutat>ungesalzene geschaelte Pistazien, fein gehackt</zutat>
          <quant>50 g</quant>
          <zutat>geriebener Parmesan</zutat>
          <quant>1 Bund</quant>
          <zutat>glattblaettrige Petersilie</zutat>
          <quant>2 Stueck</quant>
          <zutat>Kuechenschnur (je ca. 50 cm)</zutat>
          <quant>wenig </quant>
          <zutat>Pfeffer</zutat>
          <quant>1/2 TL</quant>
          <zutat>Salz</zutat>
      </Fill_Header>
      <Vinaigrette>
          <quant>150 g</quant>
          <zutat>Himbeeren, etwas zerdrueckt</zutat>
          <quant>3 EL</quant>
          <zutat>Olivenoel</zutat>
          <quant>2 EL</quant>
