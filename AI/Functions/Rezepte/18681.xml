<Rezept>
   <meta>
       <NaehrwertProPerson>876 kcal</NaehrwertProPerson>
       <Fett>53 g  </Fett>
       <Kohlenhydrate>48 g  </Kohlenhydrate>
       <Eiweiss>49 g  </Eiweiss>
   </meta>
   <einkaufliste>
      <Poulet_marinieren>
          <quant>2 </quant>
          <zutat>Knoblauchzehen, grob gehackt</zutat>
          <quant>1 </quant>
          <zutat>Schalotte, grob gehackt</zutat>
          <quant>1 </quant>
          <zutat>Limette, nur der Saft</zutat>
          <quant>1/4 dl</quant>
          <zutat>Ananassaft</zutat>
          <quant>1 EL</quant>
          <zutat>Reisessig</zutat>
          <quant>2 EL</quant>
          <zutat>Olivenoel</zutat>
          <quant>1 EL</quant>
          <zutat>Tomatenpueree</zutat>
          <quant>1/2 TL</quant>
          <zutat>Piment</zutat>
          <quant>1/2 TL</quant>
          <zutat>Paprika edelsuess</zutat>
          <quant>1/4 TL</quant>
          <zutat>Zimt</zutat>
          <quant>1 Prise</quant>
          <zutat>Muskat</zutat>
          <quant>3 </quant>
          <zutat>Pouletbruestli (je ca. 170 g)</zutat>
      </Poulet_marinieren>
      <Jalapeño-Sauce>
          <quant>200 g</quant>
          <zutat>Creme fraiche</zutat>
          <quant>1/4 Bund</quant>
          <zutat>Koriander, fein geschnitten</zutat>
          <quant>1 </quant>
          <zutat>Jalapeño, entkernt, in Wuerfeli</zutat>
          <quant>1 EL</quant>
          <zutat>Limettensaft</zutat>
          <quant>1/4 TL</quant>
          <zutat>Salz</zutat>
          <quant>wenig </quant>
          <zutat>Pfeffer</zutat>
      </Jalapeño-Sauce>
      <Poulet_braten>
          <quant>1 EL</quant>
          <zutat>Oel zum Braten</zutat>
      </Poulet_braten>
      <Nachos    >
          <quant>2 </quant>
          <zutat>gekochte Maiskolben</zutat>
          <quant>1 EL</quant>
          <zutat>Oel zum Braten</zutat>
          <quant>200 g</quant>
          <zutat>Tortilla-Chips</zutat>
          <quant>150 g</quant>
          <zutat>Gruyere oder Cheddar, grob gerieben</zutat>
      </Nachos    >
      <Gratinieren>
          <quant>1 </quant>
          <zutat>Bio-Limette, in Schnitzen</zutat>
