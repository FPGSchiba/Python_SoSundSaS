      </Suesskartoffeln_>
      <Fondue_   >
          <quant>1 </quant>
          <zutat>Knoblauchzehe, halbiert</zutat>
          <quant>350 g</quant>
          <zutat>milder Gruyere, grob gerieben</zutat>
          <quant>350 g</quant>
          <zutat>Cheddar, grob gerieben</zutat>
          <quant>2 EL</quant>
          <zutat>Maizena</zutat>
