<?xml version="1.0" encoding="UTF-8"?>
<Rezept>
   <meta>
       <NaehrwertProPerson>344 kcal</NaehrwertProPerson>
       <Fett>21 g  </Fett>
       <Kohlenhydrate>26 g  </Kohlenhydrate>
       <Eiweiss>9 g   </Eiweiss>
   </meta>
   <einkaufliste>
      <Gemuese    >
          <quant>1 kg</quant>
          <zutat>weisse Spargeln, geschaelt, halbiert</zutat>
          <quant>1 EL</quant>
          <zutat>Olivenoel</zutat>
          <quant>1/2 TL</quant>
          <zutat>Salz</zutat>
          <quant>400 g</quant>
          <zutat>Baby-Kartoffeln, halbiert</zutat>
          <quant>1 EL</quant>
          <zutat>Olivenoel</zutat>
          <quant>1/4 TL</quant>
          <zutat>Salz</zutat>
          <quant>1 Glas</quant>
          <zutat>Artischockenherzen in Oel (ca. 285 g), abgetropft, halbiert</zutat>
      </Gemuese    >
      <Spargel-Hummus>
          <quant>1 EL</quant>
          <zutat>Olivenoel</zutat>
          <quant>1 Dose</quant>
          <zutat>Kichererbsen (ca. 215 g), abgespuelt, abgetropft</zutat>
          <quant>1 EL</quant>
          <zutat>Zitronensaft</zutat>
          <quant>1 EL</quant>
          <zutat>Olivenoel</zutat>
          <quant>1/2 TL</quant>
          <zutat>Salz</zutat>
          <quant>wenig </quant>
          <zutat>Pfeffer</zutat>
      </Spargel-Hummus>
      <Bowl      >
          <quant>1 EL</quant>
          <zutat>Zitronensaft</zutat>
          <quant>1 EL</quant>
          <zutat>Olivenoel</zutat>
          <quant>2 Prisen</quant>
          <zutat>Salz</zutat>
          <quant>90 </quant>
          <zutat>Rucola</zutat>
          <quant>50 g</quant>
          <zutat>Randensprossen</zutat>
      </Bowl      >
   </einkaufliste>
   <rezept>
      <How_To    >
          <inhalt>Gemuese                        </inhalt>
      </How_To    >
      <Gemuese    >
          <inhalt>Backen                        </inhalt>
      </Gemuese    >
      <Backen    >
          <inhalt>Spargeln herausnehmen, Kartoffeln und Artischocken ca. 15 Min. fertig backen.</inhalt>
      </Backen    >
      <Spargel-Hummus>
          <inhalt>Die Haelfte der Spargeln mit den Kichererbsen, Zitronensaft und Oel puerieren, wuerzen. Hummus in Bowls verteilen.</inhalt>
      </Spargel-Hummus>
      <Bowl      >
          <inhalt>Zitronensaft und Oel verruehren, salzen. Rucola beigeben, mischen, mit den restlichen Spargeln, Kartoffeln, Artischocken und Randensprossen auf dem Hummus anrichten. </inhalt>
      </Bowl      >
   </rezept>
</Rezept>