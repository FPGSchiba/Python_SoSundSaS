   <einkaufliste>
      <Liste     >
          <quant>1 EL</quant>
          <zutat>Olivenoel</zutat>
          <quant>1/2 </quant>
          <zutat>rote Zwiebel, in feinen Streifen</zutat>
          <quant>100 g</quant>
          <zutat>Rhabarber, in feinen Scheiben</zutat>
          <quant>1 EL</quant>
          <zutat>Zucker</zutat>
          <quant>1/4 TL</quant>
          <zutat>Salz</zutat>
