<?xml version="1.0" encoding="UTF-8"?>
<Rezept>
   <meta>
       <NaehrwertProPerson>410 kcal</NaehrwertProPerson>
       <Fett>21 g  </Fett>
       <Kohlenhydrate>28 g  </Kohlenhydrate>
       <Eiweiss>27 g  </Eiweiss>
   </meta>
   <einkaufliste>
      <Teig      >
          <quant>120 g</quant>
          <zutat>Ruchmehl</zutat>
          <quant>1/2 TL</quant>
          <zutat>Salz</zutat>
          <quant>100 g</quant>
          <zutat>Magerquark</zutat>
          <quant>2 EL</quant>
          <zutat>Wasser</zutat>
          <quant>1/2 EL</quant>
          <zutat>Weissweinessig</zutat>
      </Teig      >
      <Waehe      >
          <quant>150 g</quant>
          <zutat>Magerquark</zutat>
          <quant>2 </quant>
          <zutat>Eier</zutat>
          <quant>1 dl</quant>
          <zutat>Milch</zutat>
          <quant>1/4 TL</quant>
          <zutat>Salz</zutat>
          <quant>wenig </quant>
          <zutat>Pfeffer</zutat>
          <quant>250 g</quant>
          <zutat>Kuerbis (z.B. Butternut), grob gerieben (ergibt ca. 180 g)</zutat>
          <quant>150 g</quant>
          <zutat>Raclettekaese, in Wuerfeli</zutat>
          <quant>1 TL</quant>
          <zutat>Thymianblaettchen</zutat>
          <quant>50 g</quant>
          <zutat>Bratspeck in Tranchen, in breiten Streifen</zutat>
      </Waehe      >
   </einkaufliste>
   <rezept>
      <How_To    >
          <inhalt>Teig                          </inhalt>
      </How_To    >
      <Teig      >
          <inhalt>Blindbacken                   </inhalt>
      </Teig      >
      <Blindbacken>
          <inhalt>Ofens. Herausnehmen, Backpapier und Huelsenfruechte entfernen.</inhalt>
      </Blindbacken>
      <Waehe      >
          <inhalt>Quark, Eier und Milch verruehren, wuerzen. Kuerbis, Kaese und Thymian beigeben, mischen, auf dem Teig verteilen. Speck darauf verteilen.</inhalt>
      </Waehe      >
      <Fertig_backen>
          <inhalt>Ca. 35 Min. auf der untersten Rille des auf 200 Grad vorgeheizten Ofens.</inhalt>
      </Fertig_backen>
   </rezept>
</Rezept>