   </meta>
   <einkaufliste>
      <Kalbfleischbaellchen>
          <quant>1/2 </quant>
          <zutat>Weggli, (ca. 25 g), in Wuerfeli</zutat>
          <quant>1/2 Bund</quant>
          <zutat>Petersilie, fein geschnitten</zutat>
          <quant>300 g</quant>
          <zutat>Hackfleisch (Kalb)</zutat>
          <quant>1 EL</quant>
          <zutat>Fine Food Fond de veau concentre</zutat>
          <quant>1/2 EL</quant>
          <zutat>Sesam</zutat>
          <quant>wenig </quant>
          <zutat>Pfeffer</zutat>
      </Kalbfleischbaellchen>
      <Sud_&amp;_Gemuese>
          <quant>4 </quant>
