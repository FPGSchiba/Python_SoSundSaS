<Rezept>
   <meta>
       <NaehrwertProPerson>733 kcal</NaehrwertProPerson>
       <Fett>34 g  </Fett>
       <Kohlenhydrate>77 g  </Kohlenhydrate>
       <Eiweiss>24 g  </Eiweiss>
   </meta>
   <einkaufliste>
      <Rueeblimasse>
          <quant>500 g</quant>
          <zutat>Rueebli</zutat>
          <quant>1 </quant>
          <zutat>Zwiebel</zutat>
          <quant>1 EL</quant>
          <zutat>Olivenoel</zutat>
          <quant>1/4 TL</quant>
          <zutat>Salz</zutat>
          <quant>1/2 dl</quant>
          <zutat>Wasser</zutat>
          <quant>80 g</quant>
          <zutat>Baumnusskerne</zutat>
          <quant>60 g</quant>
          <zutat>geriebener Parmesan</zutat>
          <quant>1 </quant>
          <zutat>Ei</zutat>
          <quant>3 EL</quant>
          <zutat>Paniermehl</zutat>
      </Rueeblimasse>
      <Baellchen_braten>
          <quant> </quant>
          <zutat>Oel zum Braten</zutat>
      </Baellchen_braten>
      <Linguine  >
          <quant>1 </quant>
          <zutat>Knoblauchzehe</zutat>
          <quant>1 </quant>
          <zutat>roter Peperoncino</zutat>
          <quant>1 Bund</quant>
          <zutat>glattblaettrige Petersilie</zutat>
          <quant>350 g</quant>
          <zutat>Teigwaren (z.B. Linguine)</zutat>
          <quant> </quant>
