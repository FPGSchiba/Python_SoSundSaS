<Rezept>
   <meta>
       <NaehrwertProPerson>146 kcal</NaehrwertProPerson>
       <Fett>8 g   </Fett>
       <Kohlenhydrate>6 g   </Kohlenhydrate>
       <Eiweiss>9 g   </Eiweiss>
   </meta>
   <einkaufliste>
      <Salsa     >
          <quant>1 EL</quant>
          <zutat>Olivenoel</zutat>
          <quant>1 </quant>
          <zutat>Knoblauchzehe, fein geschnitten</zutat>
          <quant>200 g</quant>
          <zutat>Lauch, in ca. 3 mm dicken Streifen</zutat>
          <quant>1 Dose</quant>
          <zutat>gehackte Tomaten (ca. 400 g)</zutat>
          <quant>1/2 TL</quant>
          <zutat>Salz</zutat>
          <quant>wenig </quant>
          <zutat>Pfeffer</zutat>
          <quant>2 EL</quant>
          <zutat>Pinienkerne, geroestet grob gehackt</zutat>
      </Salsa     >
      <Crevetten_>
          <quant> </quant>
          <zutat>Oel zum Braten</zutat>
          <quant>4 </quant>
          <zutat>Fine Food Suesswasser Crevettenschwaenze, Easy-Peel, roh, mit Schale</zutat>
          <quant>2 Zweiglein</quant>
          <zutat>Rosmarin</zutat>
          <quant>3/4 TL</quant>
          <zutat>Fleur de Sel</zutat>
      </Crevetten_>
