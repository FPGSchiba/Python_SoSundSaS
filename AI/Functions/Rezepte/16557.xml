<?xml version="1.0" encoding="UTF-8"?>
<Rezept>
   <meta>
       <NaehrwertProPerson>238 kcal</NaehrwertProPerson>
       <Fett>3 g   </Fett>
       <Kohlenhydrate>45 g  </Kohlenhydrate>
       <Eiweiss>7 g   </Eiweiss>
   </meta>
   <einkaufliste>
      <Kartoffeln_>
          <quant>400 g</quant>
          <zutat>Blaue St. Galler Kartoffeln</zutat>
          <quant> </quant>
          <zutat>Salzwasser, siedend</zutat>
          <quant>2 EL</quant>
          <zutat>Butter</zutat>
      </Kartoffeln_>
      <Teig_     >
          <quant>400 g</quant>
          <zutat>Weissmehl</zutat>
          <quant>1 1/2 TL</quant>
          <zutat>Salz</zutat>
          <quant>1/4 TL</quant>
          <zutat>Kuemmel, mit dem Moerser zerstossen</zutat>
          <quant>1/4 Wuerfel</quant>
          <zutat>Hefe (ca. 10 g), zerbroeckelt</zutat>
          <quant>1 dl</quant>
          <zutat>Wasser</zutat>
          <quant>1 EL</quant>
          <zutat>Ahornsirup</zutat>
      </Teig_     >
   </einkaufliste>
   <rezept>
      <How_To    >
          <inhalt>Kartoffeln                    </inhalt>
      </How_To    >
      <Kartoffeln_>
          <inhalt>Teig                          </inhalt>
      </Kartoffeln_>
      <Teig_     >
          <inhalt>Backen                        </inhalt>
      </Teig_     >
      <Backen    >
          <inhalt>Brattopf herausnehmen, Backpapier und Teig sofort hineingeben. Oberflaeche rautenfoermig einschneiden. Zugedeckt ca. 30 Min. backen. Deckel entfernen, ca. 20 Min. fertig backen. Brot aus dem Brattopf nehmen, auf einem Gitter auskuehlen. </inhalt>
      </Backen    >
   </rezept>
</Rezept>