<?xml version="1.0" encoding="UTF-8"?>
<Rezept>
   <meta>
       <NaehrwertProPerson>250 kcal</NaehrwertProPerson>
       <Fett>15 g  </Fett>
       <Kohlenhydrate>19 g  </Kohlenhydrate>
       <Eiweiss>5 g   </Eiweiss>
   </meta>
   <einkaufliste>
      <Dressing  >
          <quant>2 EL</quant>
          <zutat>grobkoerniger Senf</zutat>
          <quant>3 EL</quant>
          <zutat>Kraeuteressig</zutat>
          <quant>1 EL</quant>
          <zutat>Olivenoel</zutat>
          <quant>150 g</quant>
          <zutat>griechischer Jogurt nature</zutat>
          <quant>3/4 TL</quant>
          <zutat>Salz</zutat>
          <quant>wenig </quant>
          <zutat>Pfeffer</zutat>
      </Dressing  >
      <Salat     >
          <quant>300 g</quant>
          <zutat>Knollensellerie, grob gerieben</zutat>
          <quant>3 EL</quant>
          <zutat>Cranberrys, grob gehackt</zutat>
      </Salat     >
   </einkaufliste>
   <rezept>
      <Dressing  >
          <inhalt>Senf mit allen Zutaten bis und mit Pfeffer verruehren. </inhalt>
      </Dressing  >
      <Salat     >
          <inhalt>Sellerie mit dem Dressing mischen, Cranberrys darueberstreuen. </inhalt>
      </Salat     >
   </rezept>
</Rezept>