<Rezept>
   <meta>
       <NaehrwertProPerson>228 kcal</NaehrwertProPerson>
       <Fett>5 g   </Fett>
       <Kohlenhydrate>37 g  </Kohlenhydrate>
       <Eiweiss>7 g   </Eiweiss>
   </meta>
   <einkaufliste>
      <Teig_     >
          <quant>525 g</quant>
          <zutat>Weissmehl</zutat>
          <quant>1 1/2 TL</quant>
          <zutat>Salz</zutat>
          <quant>50 g</quant>
          <zutat>Puderzucker</zutat>
          <quant>1/4 Wuerfel</quant>
          <zutat>Hefe (ca. 10 g), zerbroeckelt</zutat>
          <quant>3 dl</quant>
          <zutat>Milch</zutat>
          <quant>1 </quant>
          <zutat>Eiweiss</zutat>
          <quant>50 g</quant>
          <zutat>Butter, weich</zutat>
          <quant>2 Briefchen</quant>
          <zutat>Safran</zutat>
      </Teig_     >
      <Formen    >
          <quant>1 </quant>
          <zutat>Eigelb</zutat>
          <quant>1 TL</quant>
          <zutat>Wasser</zutat>
          <quant>wenig </quant>
          <zutat>Hagelzucker, nach Belieben</zutat>
