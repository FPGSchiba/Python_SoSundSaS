   <einkaufliste>
      <Fuellung   >
          <quant>1 </quant>
          <zutat>Tomate</zutat>
          <quant>1/2 </quant>
          <zutat>rote Zwiebel</zutat>
          <quant>1 Dose</quant>
          <zutat>rote Indianerbohnen (ca. 215 g)</zutat>
          <quant>1/2 Bund</quant>
          <zutat>Koriander</zutat>
          <quant>1 EL</quant>
          <zutat>eingelegte Jalapeños in Scheiben</zutat>
          <quant>150 g</quant>
          <zutat>Mozzarella</zutat>
          <quant>100 g</quant>
          <zutat>Cheddar</zutat>
          <quant> </quant>
          <zutat>Salz, Pfeffer, nach Bedarf</zutat>
      </Fuellung   >
      <Wrap-Taschen>
          <quant>5 </quant>
          <zutat>Weizen-Tortillas</zutat>
      </Wrap-Taschen>
      <Holzkohle-_/_Gas-_/_Elektrogrill>
          <quant>1 </quant>
          <zutat>Limette</zutat>
