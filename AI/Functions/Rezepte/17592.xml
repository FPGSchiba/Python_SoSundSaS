<?xml version="1.0" encoding="UTF-8"?>
<Rezept>
   <meta>
       <NaehrwertProPerson>314 kcal</NaehrwertProPerson>
       <Fett>11 g  </Fett>
       <Kohlenhydrate>34 g  </Kohlenhydrate>
       <Eiweiss>17 g  </Eiweiss>
   </meta>
   <einkaufliste>
      <Kartoffeln>
          <quant>500 g</quant>
          <zutat>Kartoffeln festkochend, in ca. 5 mm dicken Scheiben</zutat>
          <quant>1 EL</quant>
          <zutat>Olivenoel</zutat>
          <quant>1/2 TL</quant>
          <zutat>Salz</zutat>
          <quant>wenig </quant>
          <zutat>Pfeffer</zutat>
      </Kartoffeln>
      <Huettenkaesesalat>
          <quant>200 g</quant>
          <zutat>Huettenkaese nature</zutat>
          <quant>1/2 Bund</quant>
          <zutat>glattblaettrige Petersilie, fein geschnitten</zutat>
          <quant>1/2 Bund</quant>
          <zutat>Dill, fein geschnitten</zutat>
          <quant>1 EL</quant>
          <zutat>Weissweinessig</zutat>
          <quant>1/4 TL</quant>
          <zutat>Salz</zutat>
          <quant>wenig </quant>
          <zutat>Pfeffer</zutat>
      </Huettenkaesesalat>
   </einkaufliste>
   <rezept>
      <Kartoffeln>
          <inhalt>Kartoffelscheiben mit Oel, Salz und Pfeffer mischen, auf ein mit Backpapier belegtes Blech legen. </inhalt>
      </Kartoffeln>
      <Backen    >
          <inhalt>Ca. 20 Min. in der oberen Haelfte des auf 200 Grad vorgeheizten Ofens. </inhalt>
      </Backen    >
      <Huettenkaesesalat>
          <inhalt>Huettenkaese mit Kraeutern mischen, Essig daruntermischen, wuerzen. Salat auf den Kartoffelscheiben anrichten. </inhalt>
      </Huettenkaesesalat>
   </rezept>
</Rezept>