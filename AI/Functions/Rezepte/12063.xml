<?xml version="1.0" encoding="UTF-8"?>
<Rezept>
   <meta>
       <NaehrwertProPerson>338 kcal</NaehrwertProPerson>
       <Fett>15 g  </Fett>
       <Kohlenhydrate>43 g  </Kohlenhydrate>
       <Eiweiss>5 g   </Eiweiss>
   </meta>
   <einkaufliste>
      <Quarkblaetterteig>
          <quant>200 g</quant>
          <zutat>glutenfreies Mehl (Schaer Brot-Mix)</zutat>
          <quant>1/2 TL</quant>
          <zutat>glutenfreies Backpulver (Dr. Oetker)</zutat>
          <quant>1/2 TL</quant>
          <zutat>Salz</zutat>
          <quant>100 g</quant>
          <zutat>lactosefreie Butter, in Stuecken, kalt</zutat>
          <quant>150 g</quant>
          <zutat>lactosefreier Magerquark</zutat>
      </Quarkblaetterteig>
      <Mangofuellung>
          <quant>2 </quant>
          <zutat>Mangos (je ca. 350 g), in Wuerfeli</zutat>
          <quant>100 g</quant>
          <zutat>Zucker</zutat>
          <quant>2 EL</quant>
          <zutat>Zitronensaft</zutat>
          <quant>1 </quant>
          <zutat>Vanilleschote, nur ausgekratzte Samen</zutat>
          <quant>50 g</quant>
          <zutat>Kokosraspel</zutat>
      </Mangofuellung>
      <Jalousien_zubereiten>
          <quant>1 </quant>
          <zutat>Ei, verklopft</zutat>
          <quant> </quant>
          <zutat>Puderzucker zum Bestaeuben</zutat>
      </Jalousien_zubereiten>
   </einkaufliste>
   <rezept>
      <How_To    >
          <inhalt>Quarkblaetterteig              </inhalt>
      </How_To    >
      <Quarkblaetterteig>
          <inhalt>Mangofuellung                  </inhalt>
      </Quarkblaetterteig>
      <Mangofuellung>
          <inhalt>Jalousien zubereiten          </inhalt>
      </Mangofuellung>
      <Jalousien_zubereiten>
          <inhalt>Backen: ca. 15 Min. in der Mitte des auf 220 Grad vorgeheizten Ofens. Herausnehmen, auf einem Gitter auskuehlen, mit Puderzucker bestaeuben.</inhalt>
      </Jalousien_zubereiten>
   </rezept>
