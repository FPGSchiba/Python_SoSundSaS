   <einkaufliste>
      <Liste     >
          <quant>500 g</quant>
          <zutat>Hackfleisch (Rind)</zutat>
          <quant>2 EL</quant>
          <zutat>Cornflakes</zutat>
          <quant>1 </quant>
          <zutat>Zwiebel</zutat>
          <quant>2 Zweiglein</quant>
          <zutat>Petersilie</zutat>
          <quant>1 </quant>
          <zutat>Ei</zutat>
          <quant>1 TL</quant>
          <zutat>Salz</zutat>
          <quant>wenig </quant>
