<?xml version="1.0" encoding="UTF-8"?>
<Rezept>
   <meta>
       <NaehrwertProPerson>499 kcal</NaehrwertProPerson>
       <Fett>13 g  </Fett>
       <Kohlenhydrate>83 g  </Kohlenhydrate>
       <Eiweiss>10 g  </Eiweiss>
   </meta>
   <einkaufliste>
      <Savarin   >
          <quant>200 g</quant>
          <zutat>Weissmehl</zutat>
          <quant>1 EL</quant>
          <zutat>Zucker</zutat>
          <quant>1/4 TL</quant>
          <zutat>Salz</zutat>
          <quant>1/2 Wuerfel</quant>
          <zutat>Hefe (ca. 20 g), zerbroeckelt</zutat>
          <quant>40 g</quant>
          <zutat>Butter,weich</zutat>
          <quant>2 1/4 dl</quant>
          <zutat>Milch, lauwarm</zutat>
          <quant>1 </quant>
          <zutat>Clementine, heiss abgespuelt, nur abgeriebene Schale</zutat>
      </Savarin   >
      <Sirup     >
          <quant>4 dl</quant>
          <zutat>Schwarztee</zutat>
          <quant>1/2 </quant>
          <zutat>Zimtstange</zutat>
          <quant>2 </quant>
          <zutat>Clementinen, heiss abgespuelt, wenig abgeriebene Schale und ganzer Saft</zutat>
          <quant>50 g</quant>
          <zutat>Akazienhonig</zutat>
          <quant>2 EL</quant>
          <zutat>Grand Marnier (nach Belieben)</zutat>
      </Sirup     >
      <Clementinen-Dattelsalat>
          <quant>6 </quant>
          <zutat>Clementinen</zutat>
          <quant>6 </quant>
          <zutat>Medjool-Datteln, entsteint, in Streifen</zutat>
          <quant>1 EL</quant>
          <zutat>Grand Marnier, nach Belieben</zutat>
      </Clementinen-Dattelsalat>
      <Servieren >
          <quant>1 EL</quant>
          <zutat>Creme fraiche</zutat>
      </Servieren >
   </einkaufliste>
   <rezept>
      <How_To    >
          <inhalt>Savarin                       </inhalt>
      </How_To    >
      <Savarin   >
          <inhalt>Backen                        </inhalt>
      </Savarin   >
      <Backen    >
          <inhalt>Sirup                         </inhalt>
      </Backen    >
      <Sirup     >
          <inhalt>Clementinen-Dattelsalat       </inhalt>
      </Sirup     >
      <Clementinen-Dattelsalat>
          <inhalt>5 mm dicke Scheiben schneiden, mit den Datteln und dem Grand Marnier in eine Schuessel geben. </inhalt>
      </Clementinen-Dattelsalat>
      <Servieren >
          <inhalt>Savarin kurz vor dem Servieren auf eine Platte stuerzen, Clementinen-Dattelsalat in der Mitte anrichten, mit Creme fraiche verzieren.</inhalt>
      </Servieren >
   </rezept>
</Rezept>