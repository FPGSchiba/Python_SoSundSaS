<Rezept>
   <meta>
       <NaehrwertProPerson>635 kcal</NaehrwertProPerson>
       <Fett>31 g  </Fett>
       <Kohlenhydrate>53 g  </Kohlenhydrate>
       <Eiweiss>29 g  </Eiweiss>
   </meta>
   <einkaufliste>
      <Poulet_marinieren>
          <quant>2 EL</quant>
          <zutat>Olivenoel</zutat>
          <quant>1/2 TL</quant>
          <zutat>Paprika edelsuess</zutat>
          <quant>1 TL</quant>
          <zutat>Salz</zutat>
          <quant>8 </quant>
          <zutat>Poulet-Unterschenkel (ca. 800 g)</zutat>
      </Poulet_marinieren>
      <Aprikosen-Ketchup>
          <quant>1 EL</quant>
          <zutat>Olivenoel</zutat>
          <quant>1 </quant>
          <zutat>Zwiebel, in Streifen</zutat>
          <quant>300 g</quant>
          <zutat>Aprikosen, in Stuecken</zutat>
          <quant>150 g</quant>
          <zutat>Tomaten, in Stuecken</zutat>
          <quant>2 EL</quant>
          <zutat>Wasser</zutat>
          <quant>2 EL</quant>
          <zutat>Weissweinessig</zutat>
          <quant>1 EL</quant>
          <zutat>Zucker</zutat>
          <quant>1/2 TL</quant>
          <zutat>Salz</zutat>
      </Aprikosen-Ketchup>
      <Panieren  >
          <quant>1 EL</quant>
          <zutat>Weissmehl</zutat>
          <quant>1 </quant>
          <zutat>Ei, verklopft</zutat>
          <quant>150 g</quant>
          <zutat>Cornflakes, grob zerdrueckt</zutat>
      </Panieren  >
      <Peperoni  >
          <quant>4 </quant>
          <zutat>rote Peperoni, in Stuecken</zutat>
          <quant>2 EL</quant>
          <zutat>Olivenoel</zutat>
          <quant>1/4 TL</quant>
          <zutat>Salz</zutat>
