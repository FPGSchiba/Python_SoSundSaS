   <einkaufliste>
      <Feta_und_Tomaten_>
          <quant>300 g</quant>
          <zutat>Feta</zutat>
          <quant>1 </quant>
          <zutat>Bundzwiebel mit dem Gruen, in Ringen</zutat>
          <quant>4 </quant>
          <zutat>Knoblauchzehen, halbiert</zutat>
          <quant>1 </quant>
