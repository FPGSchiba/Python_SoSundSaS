<Rezept>
   <meta>
       <NaehrwertProPerson>784 kcal</NaehrwertProPerson>
       <Fett>45 g  </Fett>
       <Kohlenhydrate>63 g  </Kohlenhydrate>
       <Eiweiss>32 g  </Eiweiss>
   </meta>
   <einkaufliste>
      <Fleisch   >
          <quant> </quant>
          <zutat>Oel zum Braten</zutat>
          <quant>500 g</quant>
          <zutat>Hackfleisch (Rind)</zutat>
          <quant>1/2 TL</quant>
          <zutat>Salz</zutat>
          <quant>wenig </quant>
          <zutat>Pfeffer</zutat>
      </Fleisch   >
      <One_Pot   >
          <quant>2 EL</quant>
          <zutat>gruene Currypaste</zutat>
          <quant>250 g</quant>
          <zutat>Parfuemreis (10 Min.), abgespuelt, abgetropft</zutat>
          <quant>350 g</quant>
          <zutat>Pak Choi, laengs in Streifen</zutat>
          <quant>5 dl</quant>
          <zutat>Kokosmilch</zutat>
          <quant>3 dl</quant>
          <zutat>Wasser</zutat>
          <quant>1 TL</quant>
          <zutat>Salz</zutat>
      </One_Pot   >
