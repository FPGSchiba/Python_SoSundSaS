<Rezept>
   <meta>
       <NaehrwertProPerson>613 kcal</NaehrwertProPerson>
       <Fett>32 g  </Fett>
       <Kohlenhydrate>58 g  </Kohlenhydrate>
       <Eiweiss>21 g  </Eiweiss>
   </meta>
   <einkaufliste>
      <Rigatoni  >
          <quant>150 g</quant>
          <zutat>Teigwaren (z.B. Rigatoni)</zutat>
          <quant> </quant>
          <zutat>Salzwasser, siedend</zutat>
          <quant>120 g</quant>
          <zutat>Jungspinat</zutat>
      </Rigatoni  >
      <Aubergine >
          <quant>1 EL</quant>
          <zutat>Olivenoel</zutat>
          <quant>1 </quant>
          <zutat>Aubergine, in Wuerfeli</zutat>
          <quant>1/4 TL</quant>
          <zutat>Salz</zutat>
          <quant>wenig </quant>
          <zutat>Pfeffer</zutat>
      </Aubergine >
      <Fertigstellen>
          <quant>2 EL</quant>
          <zutat>Olivenoel</zutat>
          <quant>100 g</quant>
