<?xml version="1.0" encoding="UTF-8"?>
<Rezept>
   <meta>
       <NaehrwertProPerson>247 kcal</NaehrwertProPerson>
       <Fett>12 g  </Fett>
       <Kohlenhydrate>29 g  </Kohlenhydrate>
       <Eiweiss>3 g   </Eiweiss>
   </meta>
   <einkaufliste>
      <Teig      >
          <quant>1 </quant>
          <zutat>rund ausgewallter Kuchenteig</zutat>
      </Teig      >
      <Apfelfuellung>
          <quant>5 </quant>
          <zutat>Aepfel, in Scheibchen</zutat>
          <quant>3 EL</quant>
          <zutat>Zucker</zutat>
          <quant>1 TL</quant>
          <zutat>Maizena</zutat>
          <quant>2 EL</quant>
          <zutat>gemahlene Haselnuesse</zutat>
          <quant>1 EL</quant>
          <zutat>Zitronensaft</zutat>
          <quant>1/4 TL</quant>
          <zutat>Zimt</zutat>
          <quant>1/4 TL</quant>
          <zutat>gemahlene Bourbon-Vanille</zutat>
      </Apfelfuellung>
      <Pie       >
          <quant>1 </quant>
          <zutat>rund ausgewallter Kuchenteig</zutat>
          <quant>1 </quant>
          <zutat>Eigelb, verklopft</zutat>
          <quant>1 EL</quant>
          <zutat>Zucker</zutat>
      </Pie       >
   </einkaufliste>
   <rezept>
      <Teig      >
          <inhalt>Teig mit dem Backpapier in die Form legen. Boden mit einer Gabel einstechen.</inhalt>
      </Teig      >
      <Apfelfuellung>
          <inhalt>Aepfel mit allen Zutaten bis und mit Vanille mischen, auf dem Teigboden verteilen. </inhalt>
      </Apfelfuellung>
      <Pie       >
          <inhalt>Teig in unterschiedlich breite Streifen schneiden. Einige Streifen zu Zoepfchen flechten. Streifen und Zoepfchen nach Belieben auf den Pie legen, gut andruecken, mit Eigelb bestreichen.</inhalt>
      </Pie       >
      <Backen    >
          <inhalt>Ca. 35 Min. in der unteren Haelfte des auf 200 Grad vorgeheizten Ofens. Herausnehmen, etwas abkuehlen, noch lauwarm servieren.</inhalt>
      </Backen    >
   </rezept>
