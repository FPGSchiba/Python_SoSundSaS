      <Quinoa    >
          <quant>1 EL</quant>
          <zutat>Butter</zutat>
          <quant>150 g</quant>
          <zutat>Quinoa</zutat>
          <quant>1 Dose</quant>
          <zutat>Red Kidney Bohnen (ca. 140 g), abgespuelt, abgetropft</zutat>
          <quant>1 dl</quant>
          <zutat>Granatapfelsaft</zutat>
