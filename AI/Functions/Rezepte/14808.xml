   <einkaufliste>
      <Granatapfel-Salsa>
          <quant>2 EL</quant>
          <zutat>Aceto balsamico</zutat>
          <quant>2 EL</quant>
          <zutat>Olivenoel</zutat>
          <quant>1 EL</quant>
          <zutat>Honig</zutat>
          <quant>1 EL</quant>
          <zutat>Granatapfelsaft</zutat>
          <quant>1/4 TL</quant>
          <zutat>Salz</zutat>
          <quant>1 </quant>
          <zutat>Granatapfel, Kerne ausgeloest</zutat>
          <quant>1 </quant>
