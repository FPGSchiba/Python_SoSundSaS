   <einkaufliste>
      <Pastateig >
          <quant>320 g</quant>
          <zutat>helles Dinkelmehl</zutat>
          <quant>4 </quant>
          <zutat>frische Eier</zutat>
          <quant>wenig </quant>
          <zutat>Weissmehl</zutat>
          <quant> </quant>
          <zutat>Salzwasser, siedend</zutat>
          <quant>1 EL</quant>
          <zutat>Olivenoel</zutat>
      </Pastateig >
      <Fisch     >
          <quant> </quant>
          <zutat>Oel zum Braten</zutat>
          <quant>600 g</quant>
          <zutat>Lachsrueckenfilet, in Wuerfeln</zutat>
          <quant>3/4 TL</quant>
          <zutat>Salz</zutat>
          <quant>wenig </quant>
          <zutat>Pfeffer</zutat>
          <quant>2 </quant>
          <zutat>Schalotten, fein gehackt</zutat>
          <quant>150 g</quant>
          <zutat>Lauch, in feinen Streifen</zutat>
          <quant>2 dl</quant>
