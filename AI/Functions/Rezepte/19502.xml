       <Eiweiss>14 g  </Eiweiss>
   </meta>
   <einkaufliste>
      <Liste     >
          <quant>1 EL</quant>
          <zutat>Olivenoel</zutat>
          <quant>1 </quant>
          <zutat>Zwiebel, fein gehackt</zutat>
          <quant>600 g</quant>
          <zutat>Blumenkohl, in Roeschen</zutat>
          <quant>300 g</quant>
          <zutat>fest kochende Kartoffeln, in Stuecken</zutat>
          <quant>2 EL</quant>
          <zutat>gelbe Currypaste (z.B. Naturaplan Curry Paste)</zutat>
          <quant>2 1/2 dl</quant>
