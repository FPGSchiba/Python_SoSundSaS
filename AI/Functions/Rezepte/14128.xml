   <einkaufliste>
      <Liste     >
          <quant>1 EL</quant>
          <zutat>Butter</zutat>
          <quant>2 </quant>
          <zutat>Schalotten, fein gehackt</zutat>
          <quant>2 cm</quant>
          <zutat>Ingwer, fein gerieben</zutat>
          <quant>1 1/5 kg</quant>
          <zutat>Kuerbis (z.B. Muscade), in Stuecken (ergibt ca. 900 g)</zutat>
          <quant>1/2 dl</quant>
          <zutat>trockener weisser Vermouth (z.B. Noilly Prat)</zutat>
