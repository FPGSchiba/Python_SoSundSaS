<Rezept>
   <meta>
       <NaehrwertProPerson>549 kcal</NaehrwertProPerson>
       <Fett>27 g  </Fett>
       <Kohlenhydrate>53 g  </Kohlenhydrate>
       <Eiweiss>35 g  </Eiweiss>
   </meta>
   <einkaufliste>
      <Sandwiches>
          <quant>8 Scheiben</quant>
          <zutat>Brot (z.B. Ruchbrot)</zutat>
          <quant>140 g</quant>
          <zutat>gekochter Schweinsbraten in Tranchen</zutat>
          <quant>140 g</quant>
          <zutat>Schinken</zutat>
          <quant>1 EL</quant>
          <zutat>Honig</zutat>
          <quant>150 g</quant>
