<?xml version="1.0" encoding="UTF-8"?>
<Rezept>
   <meta>
       <NaehrwertProPerson>539 kcal</NaehrwertProPerson>
       <Fett>38 g  </Fett>
       <Kohlenhydrate>21 g  </Kohlenhydrate>
       <Eiweiss>27 g  </Eiweiss>
   </meta>
   <einkaufliste>
      <Knuspermischung>
          <quant>40 g</quant>
          <zutat>tiefgekuehlte geschaelte Marroni, aufgetaut, fein gehackt</zutat>
          <quant>30 g</quant>
          <zutat>getrocknete Apfelschnitze, fein geschnitten</zutat>
          <quant>20 g</quant>
          <zutat>Haselnuesse, fein gehackt</zutat>
          <quant>1 EL</quant>
          <zutat>Rosmarin, fein geschnitten</zutat>
          <quant>2 TL</quant>
          <zutat>fluessiger Honig</zutat>
          <quant>1 EL</quant>
          <zutat>Rapsoel</zutat>
          <quant> </quant>
          <zutat>Salz, nach Bedarf</zutat>
      </Knuspermischung>
      <Camembert >
          <quant>250 g</quant>
          <zutat>Camembert</zutat>
          <quant>1 TL</quant>
          <zutat>fluessiger Honig</zutat>
      </Camembert >
   </einkaufliste>
   <rezept>
      <Knuspermischung>
          <inhalt>Marroni und alle Zutaten bis und mit Salz mischen. </inhalt>
      </Knuspermischung>
      <Camembert >
          <inhalt>Camembert auf ein mit Backpapier belegtes Blech legen. 1/2 der Masse auf dem Kaese verteilen, restliche Masse auf dem Blech verteilen.</inhalt>
      </Camembert >
      <Backen    >
          <inhalt>Ca. 10 Min. im auf 180 Grad vorgeheizten Ofens. Herausnehmen, Honig daruebertraeufeln, sofort servieren. Restliche Knuspermischung dazu servieren. </inhalt>
      </Backen    >
   </rezept>
</Rezept>