<?xml version="1.0" encoding="UTF-8"?>
<Rezept>
   <meta>
       <NaehrwertProPerson>459 kcal</NaehrwertProPerson>
       <Fett>18 g  </Fett>
       <Kohlenhydrate>61 g  </Kohlenhydrate>
       <Eiweiss>10 g  </Eiweiss>
   </meta>
   <einkaufliste>
      <Eintopf   >
          <quant>3 EL</quant>
          <zutat>Olivenoel</zutat>
          <quant>45 g</quant>
          <zutat>Weissmehl</zutat>
          <quant>1 </quant>
          <zutat>Zwiebel, fein gehackt</zutat>
          <quant>3 </quant>
          <zutat>Knoblauchzehen, fein gehackt</zutat>
          <quant>1 1/3 Liter</quant>
          <zutat>Gemuesebouillon</zutat>
          <quant>450 g</quant>
          <zutat>fest kochende Kartoffeln, in ca. 2 1/2 cm grossen Wuerfeln</zutat>
          <quant>2 </quant>
          <zutat>Stangensellerie, in ca. 3 cm langen Stuecken</zutat>
          <quant>2 </quant>
          <zutat>Rueebli, in ca. 1 cm dicken Scheiben</zutat>
          <quant>1 TL</quant>
          <zutat>Paprika</zutat>
          <quant>1 TL</quant>
          <zutat>getrockneter Thymian</zutat>
      </Eintopf   >
      <Dumplings >
          <quant>180 g</quant>
          <zutat>Weissmehl</zutat>
          <quant>2 TL</quant>
          <zutat>Backpulver</zutat>
          <quant>1/2 TL</quant>
          <zutat>Salz</zutat>
          <quant>1/2 TL</quant>
          <zutat>getrockneter Basilikum</zutat>
          <quant>1/2 TL</quant>
          <zutat>getrockneter Oregano</zutat>
          <quant>1 3/4 dl</quant>
          <zutat>Mandeldrink</zutat>
          <quant>2 EL</quant>
          <zutat>Olivenoel</zutat>
      </Dumplings >
   </einkaufliste>
   <rezept>
      <How_To    >
          <inhalt>Eintopf                       </inhalt>
      </How_To    >
      <Eintopf   >
          <inhalt>Dumplings                     </inhalt>
      </Eintopf   >
      <Dumplings >
          <inhalt>Aus dem Teig mit 2 kalt abgespuelten Loeffeln ca. 12 Kloesschen formen, auf das Gemuese legen. Dumplings zugedeckt bei mittlerer Hitze ca. 15 Min. daempfen.</inhalt>
      </Dumplings >
   </rezept>
</Rezept>