<?xml version="1.0" encoding="UTF-8"?>
<Rezept>
   <meta>
       <NaehrwertProPerson>151 kcal</NaehrwertProPerson>
       <Fett>6 g   </Fett>
       <Kohlenhydrate>15 g  </Kohlenhydrate>
       <Eiweiss>8 g   </Eiweiss>
   </meta>
   <einkaufliste>
      <Curry-Dip >
          <quant>125 g</quant>
          <zutat>Halbfettquark</zutat>
          <quant>1 </quant>
          <zutat>Banane (reif), zerdrueckt</zutat>
          <quant>1 TL</quant>
          <zutat>milder Curry</zutat>
          <quant>1/4 TL</quant>
          <zutat>Salz</zutat>
      </Curry-Dip >
      <Tomaten-Dip>
          <quant>140 g</quant>
          <zutat>getrocknete Tomaten in Oel, abgetropft</zutat>
          <quant>1 Scheibe</quant>
          <zutat>Toastbrot, in Stuecken</zutat>
      </Tomaten-Dip>
      <Thon-Dip  >
          <quant>1 Dose</quant>
          <zutat>rosa Thon in Oel (ca. 80 g), abgetropft</zutat>
          <quant>125 g</quant>
          <zutat>Frischkaese</zutat>
          <quant>1 EL</quant>
          <zutat>Schnittlauch, fein geschnitten</zutat>
          <quant> </quant>
          <zutat>Salz, nach Bedarf</zutat>
      </Thon-Dip  >
      <Gemuese-Sticks>
          <quant>1 </quant>
          <zutat>Gurke, in Staengeln</zutat>
          <quant>1 </quant>
          <zutat>rote Peperoni, in Staengeln</zutat>
          <quant>8 </quant>
          <zutat>Bundrueebli</zutat>
          <quant>1 </quant>
          <zutat>Kohlrabi, in Staengeln</zutat>
      </Gemuese-Sticks>
   </einkaufliste>
   <rezept>
      <Curry-Dip >
          <inhalt>Quark und Banane verruehren, wuerzen. </inhalt>
      </Curry-Dip >
      <Tomaten-Dip>
          <inhalt>Tomaten und Brot fein puerieren. </inhalt>
      </Tomaten-Dip>
      <Thon-Dip  >
          <inhalt>Thon mit einer Gabel zerdruecken, mit Frischkaese und Schnittlauch mischen, salzen. </inhalt>
      </Thon-Dip  >
      <Gemuese-Sticks>
          <inhalt>Dips je in 2 Glaeser fuellen, Gemuese hineinstellen.</inhalt>
      </Gemuese-Sticks>
   </rezept>
</Rezept>